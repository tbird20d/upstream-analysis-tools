#!/bin/sh
#
# make-areas - make a set of area-<area>.diff files, from base.diff
#

diffinfo -p /android -n area-android.diff base.diff
diffinfo -p barcode -n area-barcode.diff base.diff
diffinfo -p battery -n area-bat.diff base.diff
diffinfo -p bif -n area-bif.diff base.diff
diffinfo -p block -n area-block.diff base.diff
diffinfo -p bluetooth -n area-bluetooth.diff base.diff
diffinfo -p camera -n area-camera.diff base.diff
diffinfo -p coresight -n area-coresight.diff base.diff
diffinfo -p cpufreq\|cpuidle\|devfreq -n area-cpufreq.diff base.diff
diffinfo -p crypto -n area-crypto.diff base.diff
diffinfo -p defconfig -n area-defconfig.diff base.diff
diffinfo -p diag -n area-diag.diff base.diff
diffinfo -p devicetree/bindings -n area-dt-bindings.diff base.diff
diffinfo -p dtc -n area-dtc.diff base.diff
diffinfo -p boot/dts -n area-dts.diff base.diff
diffinfo -p net/ethernet -n area-ethernet.diff base.diff
diffinfo -p felica -n area-felica.diff base.diff
diffinfo -p firmware -n area-firmware.diff base.diff
diffinfo -p fs/ -n area-fs.diff base.diff
diffinfo -p gpio -n area-gpio.diff base.diff
diffinfo -p gpu -n area-gpu.diff base.diff
diffinfo -p gud/ -n area-gud.diff base.diff
diffinfo -p hwmon -n area-hwmon.diff base.diff
diffinfo -p i2c -n area-i2c.diff base.diff
diffinfo -p iio/ -n area-iio.diff base.diff
diffinfo -p drivers/input -n area-input.diff base.diff
diffinfo -p intel-mid -n area-intel-mid.diff base.diff
diffinfo -p iommu -n area-iommu.diff base.diff
diffinfo -p msm/ipa -n area-ipa.diff base.diff
diffinfo -p irda -n area-irda.diff base.diff
diffinfo -p kernel/ -n area-kernel.diff base.diff
diffinfo -p leds -n area-leds.diff base.diff
diffinfo -p mach-exynos -n area-mach-exynos.diff base.diff
diffinfo -p mach-msm -n area-mach-msm.diff base.diff
diffinfo -p max77 -n area-max77.diff base.diff
diffinfo -p media -n area-media.diff base.diff
diffinfo -p mfd -n area-mfd.diff base.diff
diffinfo -p ^mm/\|/mm/ -n area-mm.diff base.diff
diffinfo -p /mmc -n area-mmc.diff base.diff
diffinfo -p motor -n area-motor.diff base.diff
diffinfo -p /mtd -n area-mtd.diff base.diff
diffinfo -p arch/arm/mvp -n area-mvp.diff base.diff
diffinfo -p netfilter -n area-netfilter.diff base.diff
diffinfo -p perf -n area-perf.diff base.diff
diffinfo -p pinctrl -n area-pinctrl.diff base.diff
diffinfo -p power -n area-power.diff base.diff
diffinfo -p regulator -n area-reg.diff base.diff
diffinfo -p security\|secfilter -n area-security.diff base.diff
diffinfo -p sensorhub -n area-sensorhub.diff base.diff
diffinfo -p sensors -n area-sensors.diff base.diff
diffinfo -p slimbus -n area-slimbus.diff base.diff
diffinfo -p sound\|audio -n area-sound.diff base.diff
diffinfo -p msm/sps -n area-sps.diff base.diff
diffinfo -p thermal -n area-thermal.diff base.diff
diffinfo -p trace -n area-trace.diff base.diff
diffinfo -p tty -n area-tty.diff base.diff
diffinfo -p usb -n area-usb.diff base.diff
diffinfo -p video -n area-video.diff base.diff
diffinfo -p wireless\|wlan -n area-wireless.diff base.diff
diffinfo -p oneseg -n area-oneseg.diff base.diff
diffinfo -p tdmb -n area-tdmb.diff base.diff
diffinfo -p prima -n area-prima.diff base.diff
diffinfo -p nfc -n area-nfc.diff base.diff
diffinfo -p rtc -n area-rtc.diff base.diff
diffinfo -p debug_tools -n area-debug_tools.diff base.diff
diffinfo -p hsi -n area-hsi.diff base.diff
diffinfo -p intel-mid -n area-intel-mid.diff base.diff
diffinfo -p iqvandroid -n area-iqvandroid.diff base.diff
diffinfo -p sep54 -n area-sep54.diff base.diff
diffinfo -p tp2 -n area-tp2.diff base.diff
